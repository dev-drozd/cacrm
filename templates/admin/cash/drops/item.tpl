<tr class="tr {class}" id="drop_{id}">
	<td class="td" data-label="ID:">{id}</td>
	<td class="td" data-label="Name:"><a href="/users/view/{user-id}" onclick="Page.get(this.href); return false;">{name}</a></td>
	<td class="td" data-label="Store:">{object_name}</td>
	<td class="td" data-label="Date:">{date}</td>
	<td class="td" data-label="Amount:">{amount}</td>
	<td class="td" data-label="Confirm date:">{confirm-date}</td>
	<td class="td" data-label="Confirm staff:">{confirm-staff}</td>
	<td class="td" data-label="Confirmed:">{confirmed}</td>
</tr>