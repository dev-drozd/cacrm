<div class="sUser[deleted] deleted[/deleted]" id="user_{id}">
	<a href="/users/view/{uid}" onclick="Page.get(this.href); return false;" class="uInfo">
		<div class="uInfo">
			{name} {lastname}<br>
			<i>{phone}</i>
		</div>
	</a>
	<div class="pInfo">
		<b>{object}</b>
	</div>
	<div class="pInfo">
		<b>Agent:</b><br>
		<span>{first_name} {last_name}</span>
	</div>
	<div class="pInfo">
		<b>Note:</b><br>
		{note}
	</div>
</div>