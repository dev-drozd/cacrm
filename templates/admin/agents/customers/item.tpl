<div class="sUser[deleted] deleted[/deleted]" id="user_{id}">
	<a href="/users/view/{id}" onclick="Page.get(this.href); return false;" class="uInfo">
		<div class="uInfo">
			{name} {lastname}
		</div>
	</a>
	<div class="pInfo">
		{phone}
	</div>
	<div class="pInfo">
		<b>Registered: </b><br>
		{reg_date}
	</div>
	<div class="pInfo">
		<b>Agent:</b><br>
		<span>{first_name} {last_name}</span>
	</div>
</div>