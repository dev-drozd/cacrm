<div class="tr dev" id="note_{id}">
	<div class="td w10">
		{date}
	</div>
	<div class="td w100">
		<a href="/users/view/{staff-id}">{staff-name} {staff-lastname}</a>
	</div>
	<div class="td">
		{note}
	</div>
	<div class="td w5">
		<a href="javascript:user.delNote({id});"><span class="fa fa-times"></span></a>
	</div>
</div>